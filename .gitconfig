[user]
    email  = angel.campoverde@cern.ch
    name   = Angel Campoverde
[pull]
    rebase = false
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
[alias]
    ac=add .
    br=branch
    cl=clone
    cm=commit
    co=checkout
    po=push origin
    rs=reset
    st=status

    pom=push    origin master
    poh=push -u origin HEAD 
    cob=checkout -b

[init]
	defaultBranch = master
[http]
	proxy = socks5h://127.0.0.1:8080
[https]
	proxy = socks5h://127.0.0.1:8080
